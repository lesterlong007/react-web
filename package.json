{
  "name": "react-web",
  "version": "1.0.0",
  "description": "A react web application",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "start": "node scripts/start.js",
    "build:dev": "webpack --config ./scripts/build.js --env dev",
    "build:sit": "webpack --config ./scripts/build.js --env sit",
    "build:uat": "webpack --config ./scripts/build.js --env uat",
    "build:prod": "webpack --config ./scripts/build.js --env prod",
    "server": "node server/index.js",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx src/",
    "analyze": "cross-env ANALYZE=1 npm run build:prod",
    "mock": "nodemon ./mock/server.js",
    "prettier": "prettier --write ./src/**/**/*.{js,jsx,ts,tsx,json,scss}"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js.jsx,ts,tsx,json,html,scss,sass,css,md}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js.jsx,ts,tsx}": "npm run lint"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
